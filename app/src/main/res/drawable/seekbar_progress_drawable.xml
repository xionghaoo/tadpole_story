<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!--定义seekbar滑动条的底色-->
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/_10dp" />
            <solid android:color="@color/color_6A6F80"/>
<!--            <gradient-->
<!--                android:angle="270"-->
<!--                android:centerColor="#eeeff3"-->
<!--                android:centerY="0.75"-->
<!--                android:endColor="#eeeff3"-->
<!--                android:startColor="#eeeff3" />-->
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <scale android:scaleWidth="100%">
            <shape android:shape="rectangle">
                <corners android:radius="@dimen/_10dp" />
                <solid android:color="@color/color_9AA5AA"/>
            </shape>
        </scale>
    </item>
    <!--定义seekbar滑动条进度颜色-->
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="@dimen/_10dp"/>
                <gradient android:startColor="@color/color_51CBFF" android:endColor="@color/color_4121FF"/>
            </shape>
        </clip>
    </item>
</layer-list>